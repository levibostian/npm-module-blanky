{
  "name": "@levibostian/npm-module-blanky",
  "version": "0.1.0",
  "description": "replace me",
  "scripts": {
    "test": "npx jest",
    "format": "npx prettier --write \"{app,tests}/**/*.ts\"",
    "build": "rm -rf dist && npx tsc && find dist -type f -name '*.test.js*' -delete && find dist -type f -name '*.test.d.ts' -delete && cp package*.json dist/ && cp LICENSE dist/ && cp README.md dist/",
    "compile": "npm run build && npm publish dist/ --dry-run",
    "docs": "rm -rf docs && npx typedoc app",
    "lint": "npx eslint --fix --ext ts,js . && npx tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/levibostian/npm-module-blanky.git"
  },
  "keywords": [
    "boilerplate"
  ],
  "author": "Levi Bostian <levi.bostian@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/levibostian/npm-module-blanky/issues"
  },
  "homepage": "https://github.com/levibostian/npm-module-blanky#readme",
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,json,css,md}": [
      "npm run format"
    ]
  },
  "prettier": "@levibostian/prettier-config",
  "devDependencies": {
    "@levibostian/eslint-config-node": "^1.0.2",
    "@levibostian/jest-config": "^1.0.1",
    "@levibostian/prettier-config": "^1.0.0",
    "@levibostian/tsconfigs": "^1.1.0",
    "@tsconfig/node12": "^1.0.7",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.7",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "codecov": "^3.8.1",
    "eslint": "^7.13.0",
    "eslint-plugin-jest": "^24.1.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.4",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.5"
  }
}
